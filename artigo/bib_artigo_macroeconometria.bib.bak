  Keywords                 = {alocação de ativos, asset allocation, carteiras de variância mínima, gestão quantitativa de investimentos, minimum variance portfolios, otimização de carteiras, portfolio optimization, quantitative asset management, shrinkage},
  Language                 = {pt},
  Url                      = {http://bibliotecadigital.fgv.br/ojs/index.php/rbfin/article/view/5830},
  Urldate                  = {2017-01-02}
}

