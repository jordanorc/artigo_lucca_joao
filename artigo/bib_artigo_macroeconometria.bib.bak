@Article{herwartz_generalized_2011,
  Title                    = {Generalized least squares estimation for cointegration parameters under conditional heteroskedasticity},
  Author                   = {Herwartz, Helmut and Lütkepohl, Helmut},
  Journal                  = {Journal of Time Series Analysis},
  Year                     = {2011},

  Month                    = may,
  Number                   = {3},
  Pages                    = {281--291},
  Volume                   = {32},

  Abstract                 = {In the presence of generalized conditional heteroscedasticity (GARCH) in the residuals of a vector error correction model (VECM), maximum likelihood (ML) estimation of the cointegration parameters has been shown to be efficient. On the other hand, full ML estimation of VECMs with GARCH residuals is computationally difficult and may not be feasible for larger models. Moreover, ML estimation of VECMs with independently identically distributed residuals is known to have potentially poor small sample properties and this problem also persists when there are GARCH residuals. A further disadvantage of the ML estimator is its sensitivity to misspecification of the GARCH process. We propose a feasible generalized least squares estimator which addresses all these problems. It is easy to compute and has superior small sample properties in the presence of GARCH residuals.},
  Doi                      = {10.1111/j.1467-9892.2010.00698.x},
  File                     = {Full Text PDF:C\:\\Users\\Lucca\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\o8bk9xf9.default\\zotero\\storage\\K776NE4Q\\Herwartz e Lütkepohl - 2011 - Generalized least squares estimation for cointegra.pdf:application/pdf;Snapshot:C\:\\Users\\Lucca\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\o8bk9xf9.default\\zotero\\storage\\BR8Q8823\\abstract.html:text/html},
  ISSN                     = {1467-9892},
  Keywords                 = {C32, cointegration, maximum likelihood estimation, multi-variate GARCH, reduced rank estimation, Vector autoregressive process, vector error correction model},
  Language                 = {en},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1111/j.1467-9892.2010.00698.x/abstract},
  Urldate                  = {2017-02-03}
}

@Article{jarque_test_1987,
  Title                    = {A {Test} for {Normality} of {Observations} and {Regression} {Residuals}},
