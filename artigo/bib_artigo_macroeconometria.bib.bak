  Keywords                 = {Biofuels, Price volatility interactions, Semiparametric GARCH, sugar},
  Shorttitle               = {Volatility spillovers between food and energy markets},
  Url                      = {//www.sciencedirect.com/science/article/pii/S0140988311000867},
  Urldate                  = {2017-01-22}
}

