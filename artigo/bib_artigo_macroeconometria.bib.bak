@Article{perlin_researchers_2015,
  Title                    = {The researchers, the publications and the journals of {Finance} in {Brazil}: {An} analysis based on resumes from the {Lattes} platform},
  Author                   = {Perlin, Marcelo Scherer and Santos, André Portela},
  Journal                  = {Brazilian Review of Finance},
